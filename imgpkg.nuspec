    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://cdn.statically.io/gh/adriens/chocolatey-kbld/main/img/CarvelLogo.png</iconUrl>
    <packageSourceUrl>https://github.com/adriens/chocolatey-imgpkg</packageSourceUrl>
    <releaseNotes>https://github.com/vmware-tanzu/carvel-imgpkg/releases/tag/v0.6.1</releaseNotes>
    <docsUrl>https://github.com/vmware-tanzu/carvel-imgpkg/tree/develop/docs</docsUrl>
